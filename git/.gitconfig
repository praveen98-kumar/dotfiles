[init]
	defaultBranch = main
[user]
	name = Praveen Kumar Koram
	email = pkkoram98@gmail.com
[core]
  pager = delta

[interactive]
  diffFilter = delta --color-only
[add.interactive]
  useBuiltin = false

[delta]
  navigate = true
  light = false

[merge]
  conflictstyle = diff3

[diff]
  colorMoved = default
[alias]
	st = status -s
	lg = log --oneline --graph --decorate --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
	br = branch
